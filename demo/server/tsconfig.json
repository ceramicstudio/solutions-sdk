{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "lib": ["DOM", "ES2022"],
    "resolveJsonModule": false,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "preserveConstEnums": true,
    "strict": true,
    "rootDir": "./",
    "outDir": "dist",
    "esModuleInterop": true,
    "downlevelIteration": true,
  }
}