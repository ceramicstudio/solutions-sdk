{
  "name": "@composesolutions/points",
  "version": "0.1.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/solutions",
    "directory": "libraries/points"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc --strip-leading-paths",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly --skipLibCheck",
    "build": "pnpm build:clean && pnpm build:types && pnpm build:js",
    "lint": "eslint src --fix",
    "prepare": "pnpm build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@ceramicnetwork/http-client": "^5.3.0",
    "@composedb/loader": "^0.7.1",
    "@composesolutions/points-composite": "workspace:^"
  },
  "devDependencies": {
    "@ceramicnetwork/common": "^5.2.0",
    "@ceramicnetwork/stream-model-instance": "^4.3.0",
    "@composedb/types": "^0.7.1",
    "dids": "^5.0.2"
  }
}
